{
  "name": "rnstorybook",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "./tools/setup.sh clean",
    "setup": "./tools/setup.sh setup",
    "updatedeps": "./tools/setup.sh updatedeps",
    "android": "npx prestorybook && npx react-native run-android",
    "ios": "npx prestorybook && npx react-native run-ios",
    "start": "npx react-native start",
    "test": "npx jest",
    "lint": "npx eslint .",
    "prestorybook": "npx rnstl",
    "build-storybook": "npx build-storybook",
    "storybook": "npx start-storybook -p 7007",
    "build-react": "npx webpack --mode production",
    "start-react": "npx webpack-dev-server --config ./webpack.config.js --mode development",
    "start-storybook-web": "./node_modules/@storybook/react/bin/index.js",
    "build-storybook-web": "./node_modules/@storybook/react/bin/build.js",
    "storybook-web": "npm run start-storybook-web"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-native": "0.64.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.12.5",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react": "^5.3",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "dotenv-webpack": "^7.0.3",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react-native": "^3.8.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-dom": "17.0.2",
    "react-native-codegen": "^0.0.7",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-web": "^0.17.1",
    "react-test-renderer": "17.0.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
